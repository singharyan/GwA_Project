      "tailwindcss": "^1.3.5",
      "ws": "^7.2.3"
   },
   "dependencies": {
      "@beyonk/svelte-carousel": "^2.4.0",
      "firebase": "^7.14.2",
      "svelte-feather-icons": "^3.2.2",
      "sveltefire": "^0.1.4"
   }
